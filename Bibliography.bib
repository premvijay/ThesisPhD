@article{osti6457593,
	title        = {Astrophysical bounds on the mass of heavy stable neutral leptons},
	author       = {{Zel'dovich}, Y B and {Klypin}, A A and Khlopov, M Y and Chechetkin, V M},
	year         = 1980,
	month        = 5,
	journal      = {Sov. J. Nucl. Phys. (Engl. Transl.); (United States)},
	volume       = 31,
	url          = {https://www.osti.gov/biblio/6457593},
	abstractnote = {Analytical and numerical calculations show that heavy neutral stable leptons are carried along by the collapsing matter during the formation of galaxies and possibly stars as well. The condensation in galaxies and stars results in appreciable annihilation of leptons and antileptons. Modern observations of cosmic-ray and g-ray fluxes establish a limit m/sub n/> or approx. =100 GeV for the mass of neutral leptons, since annihilation of neutral leptons produces g rays and cosmic rays. The obtained bound, in conjunction with ones established earlier, precludes the existence of stable neutral leptons (neutrinos) with m/sub n/>30 eV.}
}
